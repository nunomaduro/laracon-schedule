{
    "name": "nunomaduro/laracon-schedule",
    "description": "Get the Laracon Schedule from the terminal.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "laracon"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nunomaduro/laracon-schedule/issues",
        "source": "https://github.com/nunomaduro/laracon-schedule"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "laravel-zero/framework": "^9.0",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/laracon-schedule"]
}
